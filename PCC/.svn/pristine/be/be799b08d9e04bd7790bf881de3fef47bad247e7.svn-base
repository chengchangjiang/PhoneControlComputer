<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.pcc"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="19" />
        
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.CHANGE_WIFI_MULTICAST_STATE" />
    
    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:theme="@android:style/Theme.NoTitleBar">
        <activity
            android:name=".Splash"
            android:launchMode="singleTop"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".FirstLoginActivity"></activity>
     	<activity android:name=".SDconcult"/>
    	<activity android:name=".SetShutdownTime"/>
    	<activity android:name=".CancelShutdown"/>
    	<activity android:name=".FirstLogin"/>
    	<activity android:name=".MainActivity"/>
    	<activity android:name=".AirControlActivity"></activity>
    	<activity android:name=".MusicActivity"/>
    	<activity 
    	    android:theme="@android:style/Theme.NoTitleBar.Fullscreen"
    	    android:name=".Desktop"
    	    android:screenOrientation = "landscape"/>
    	<activity android:name="com.example.pcc.CameraActivity"/>
    	<activity android:name="com.example.pcc.BaseActivity"/>
    	<activity android:name="com.example.pcc.PhotoSendActivity"/>
    	<activity android:name="com.example.pcc.PPTControlActivity"
    	android:screenOrientation = "landscape"/>
    	<activity android:name=".MoreActivity"/>
    </application>
    
</manifest>
