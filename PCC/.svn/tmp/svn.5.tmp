<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    tools:context="com.pcc.ray.ppttest.MainActivity">
    <ImageView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:scaleType="fitXY"
        android:id="@+id/printf_ppt_imageview"/>

    <ImageButton
        android:id="@+id/next_ppt_button"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="50dp"
        android:scaleType="fitXY"
        android:src="@drawable/last_ppt_button" />

    <ImageButton
        android:id="@+id/previous_ppt_button"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_alignParentBottom="true"
        android:layout_marginLeft="58dp"
        android:layout_toRightOf="@+id/next_ppt_button"
        android:scaleType="fitXY"
        android:src="@drawable/previous_ppt_button" />

</RelativeLayout>
